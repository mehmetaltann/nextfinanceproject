{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "features/Calculation/CalcMain.js", "features/Calculation/components/BanksCalculateSheets.tsx", "features/Calculation/components/BaseCalculateSheet.tsx", "features/Calculation/components/TotalCalculateSheets.tsx", "app/calculation/page.tsx", "features/Calculation/CalcMain.tsx", "components/FormElements/FormDatePicker.tsx"],
  "exclude": ["node_modules"]
}
